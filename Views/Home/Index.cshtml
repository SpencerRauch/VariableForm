@{
    ViewData["Title"] = "Home Page";
}
@{ List<int> DynamicData= new List<int>{3,4,5,6,7,8,9};}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <form asp-action="Process">
        <input type="hidden" name="PrimeId" value="4">
        @foreach (int num in DynamicData)
        {
            <div>
                <label for="@num">Item @num</label>
                <input type="checkbox" name="vals" value="@num" id="@num">
            </div>
        }
        <button>Submit</button>
    </form>
</div>
